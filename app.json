{
  "expo": {
    "name": "gomotors",
    "slug": "gomotors",
    "version": "1.0.0",
    "splash": {
      "image": "./src/assets/splash-goMotors.png",
      "resizeMode": "contain",
      "backgroundColor": "#202024"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-firebase/app"
    ],
    "android": {
      "package": "com.mateuspp.gomotors",
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.mateuspp.gomotors",
      "googleServicesFile": "./GoogleService-Info.plist"
    }
  }
}
